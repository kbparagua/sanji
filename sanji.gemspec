Gem::Specification.new do |s|
  s.name = 'sanji'
  s.version = '1.0.0.pre.1'
  s.description = 'rails application generator'
  s.summary = 'rails application generator'
  s.authors = ['Karl Paragua']
  s.email = 'kb.paragua@gmail.com'
  s.homepage = 'https://www.github.com/kbparagua/sanji'
  s.files = Dir["{lib}/**/*.rb", "bin/*", "LICENSE", "*.md"]
  s.require_path = 'lib'
  s.executables << 'sanji'
  s.license = 'MIT'
end
